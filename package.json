{
  "name": "agoda-design-toolkit-2",
  "version": "2.3.2",
  "engines": {
    "sketch": ">=3.0"
  },
  "skpm": {
    "name": "Agoda Design Toolkit",
    "manifest": "src/manifest.json",
    "main": "Agoda-Design-Toolkit.sketchplugin",
    "assets": [
      "assets/**/*"
    ]
  },
  "scripts": {
