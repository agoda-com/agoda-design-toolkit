  },
  "devDependencies": {
    "@skpm/builder": "^0.5.16"
  },
  "author": "Agoda",
  "icon": "icons/icon.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/agoda-com/agoda-design-toolkit.git"
  },
  "dependencies": {
    "@skpm/fs": "^0.2.5",
    "mocha-js-delegate": "^0.1.1",
    "npm": "^6.9.0",
    "sketch-module-user-preferences": "^1.0.2",
    "sketch-module-web-view": "^1.2.3"
  }
}
